 import java.util.Scanner;
 class Main
 {
 public static void main(String args[])
 {
 int num;
 int temp;
 int sum = 0;
 Scanner sc= new Scanner(System.in);
 System.out.print("Enter a number: ");
 num = sc.nextInt();
 while (sum != 1 && sum != 4)
 {
 sum = 0;
 while (num > 0)
 {
 temp = num % 10;
 sum += (temp * temp);
 num = num / 10;
 }
 num = sum;
 }
 if (sum == 1)
 {
 System.out.print("Happy Number");
 }
 else
 {
 System.out.print("UnHappy Number");
 }
 }
 }
