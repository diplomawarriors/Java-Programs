import java.util.Scanner;
 class Main
 {
 public static void main(String args[])
 {
 boolean flag = false;
 Scanner sc= new Scanner(System.in);
 System.out.print("Enter a Number (upper limit): ");
 int limit = sc.nextInt();
 for (int i = 0;i <= limit;i++)
 {
 flag = false;
 for (int n = 0;n <= i;n++)
 {
 if (n* (n + 1) == i)
 {
 flag = true;
 break;
 }
 }
 if (flag)
 {
 System.out.println(i+"");
 }
 }
 }
 }

