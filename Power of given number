 import java.util.Scanner;
 class Main
 {
 public static void main(String args[])
 {
 int pow;
 int num;
 int i = 1;
 int sum = 1;
 Scanner sc=new Scanner(System.in);
 System.out.print("Enter a number: ");
 num = sc.nextInt();
 System.out.print("Enter power: ");
 pow = sc.nextInt();
 while (i <= pow)
 {
 sum = sum * num;
 i++;
 }
 System.out.print("\n"+num+" to the power "+pow+" is:"+sum);
 }
 }

